      <parameterlist>
        <parameter>D1_1_2</parameter>
      </parameterlist>
      <limittype>String</limittype>
      <spec>
        <limit>OK</limit>
      </spec>
    </step>
    <step>
      <stepnum>1.4</stepnum>
      <stepname>Enable the continuous mode for channel1.</stepname>
      <stepdescription>
      </stepdescription>
      <functionname>ExecuteNetworkAnalyzerCommand</functionname>
      <parameterlist>
        <parameter>:INIT1:CONT ON</parameter>
      </parameterlist>
      <limittype>String</limittype>
      <spec>
        <limit>OK</limit>
      </spec>
    </step>
    <step>
      <stepnum>1.5</stepnum>
