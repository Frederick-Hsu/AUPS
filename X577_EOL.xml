<?xml version="1.0" encoding="UTF-8"?>
<sequence>
  <block>
    <blocknum>B1</blocknum>
    <blockname>Resistor measurement.</blockname>
    <step>
      <stepnum>1.1</stepnum>
      <stepname>Initialize the Keysight Digitial Multimeter 34461A.</stepname>
      <stepdescription>Through the ethernet to initialize and connect to DMM.</stepdescription>
      <functionname>InitializeDigitalMultimeter</functionname>
      <parameterlist>
        <parameter>USB0::0x2A8D::0x1301::MY57204070::0::INSTR</parameter>
      </parameterlist>
      <limittype>String</limittype>
      <spec>
        <limit>Keysight Technologies,34461A,MY57204070,A.02.14-02.40-02.14-00.49-03-02</limit>
      </spec>
    </step>
    <step>
      <stepnum>1.2</stepnum>
      <stepname>Measure the 2wires resistor</stepname>
      <stepdescription>Over the 2 wires, measure the resistance value of your resstor</stepdescription>
      <functionname>MeasureResistorOver2Wires</functionname>
      <parameterlist />
      <limittype>Numerical</limittype>
      <spec>
        <limit>1800.00</limit>
        <limit>2000.00</limit>
        <limit>2200.00</limit>
      </spec>
    </step>
    <step>
      <stepnum>1.3</stepnum>
      <stepname>Measure the 4wires resistor</stepname>
      <stepdescription>Over the 4 wires, measure the resistance value of your resstor</stepdescription>
      <functionname>MeasureResistorOver4Wires</functionname>
      <parameterlist />
      <limittype>Numerical</limittype>
      <spec>
        <limit>1800.00</limit>
        <limit>2000.00</limit>
        <limit>2200.00</limit>
      </spec>
    </step>
    <step>
      <stepnum>1.4</stepnum>
      <stepname>Close the Digitial Multi Meter.</stepname>
      <stepdescription>Close the session of communicating with Keysight DMM.</stepdescription>
      <functionname>CloseDMM</functionname>
      <parameterlist />
      <limittype>String</limittype>
      <spec>
        <limit>OK</limit>
      </spec>
    </step>
  </block>
</sequence>